{
  "kind": "collectionType",
  "collectionName": "warehouses",
  "info": {
    "singularName": "warehouse",
    "pluralName": "warehouses",
    "displayName": "Warehouse",
    "description": "Supplier warehouses and storage locations"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 50
    },
    "location": {
      "type": "string",
      "maxLength": 500
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "supplier": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::supplier.supplier",
      "inversedBy": "warehouses"
    },
    "warehouseStock": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::warehouse-stock.warehouse-stock",
      "mappedBy": "warehouse"
    }
  }
}
