{
  "kind": "collectionType",
  "collectionName": "suppliers",
  "info": {
    "singularName": "supplier",
    "pluralName": "suppliers",
    "displayName": "Supplier",
    "description": "Product suppliers and retail store"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 255
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 50
    },
    "contactEmail": {
      "type": "email"
    },
    "contactPhone": {
      "type": "string",
      "maxLength": 50
    },
    "isRetailStore": {
      "type": "boolean",
      "default": false
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "warehouses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::warehouse.warehouse",
      "mappedBy": "supplier"
    },
    "supplierProducts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::supplier-product.supplier-product",
      "mappedBy": "supplier"
    }
  }
}
